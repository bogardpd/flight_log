<h2>Boarding Pass Data</h2>

<p><%= link_to "JSON output", show_boarding_pass_json_path(boarding_pass.raw) %></p>

<div id="flight-boarding-pass-data">
  <% boarding_pass.ordered_groups.each do |group| %>
    <h3><%= group[:title] %></h3>
    <%= render partial: "bcbp_group", locals: {group_fields: group[:fields]} %>
  <% end %>
</div>